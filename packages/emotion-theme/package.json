{
  "name": "@team-comit/emotion-theme",
  "version": "0.0.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:clean && yarn build:type && yarn build:bundle",
